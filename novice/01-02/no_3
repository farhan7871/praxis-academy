var x = 8;
var y = 1;
var z = 2;


if (x > 0 && y > 0 && z > 0) {
    alert("Tandanya adalah +");
}
else if (x < 0 && y < 0 && z > 0) {
    console.log("Tandanya adalah +")
}
else if (x > 0 && y < 0 && z < 0) {
    console.log("Tandanya adalah +");
}
else if (x < 0 && y > 0 && z < 0) {
    console.log("Tandanya adalah +");
} else {
    console.log("Tandanya adalah -");
}
